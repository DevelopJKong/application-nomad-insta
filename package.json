      "@apollographql/graphql-language-service-utils": {
         "graphql": "$graphql"
      }
   },
   "devDependencies": {
      "@babel/core": "^7.20.0",
      "@graphql-codegen/cli": "5.0.2",
      "@graphql-codegen/client-preset": "4.2.5",
